{
  "name": "react-native-nativewind-grid",
  "version": "0.1.0",
  "description": "Nativewind grid utiliy",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.tsx",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./plugin": {
      "source": "./plugin/index.js",
      "default": "./plugin/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "yarn workspace react-native-nativewind-grid-example",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "test": "jest",
    "setup": "bob build && cd example/ && adb uninstall com.example || true && yarn start --reset-cache & sleep 5 && yarn android && cd ..",
    "release": "release-it --only-version"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamshrikantjha/react-native-nativewind-grid.git"
  },
  "author": "Shrikant Jha <shrikantjha11@gmail.com> (https://github.com/iamshrikantjha)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamshrikantjha/react-native-nativewind-grid/issues"
  },
  "homepage": "https://github.com/iamshrikantjha/react-native-nativewind-grid#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/eslint-config": "0.83.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.12",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "lefthook": "^2.0.3",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-builder-bob": "^0.40.17",
    "release-it": "^19.0.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "nativewind": ">=4.0.0",
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": ">=3.0.0",
    "react-native-safe-area-context": ">=4.0.0"
  }
}
